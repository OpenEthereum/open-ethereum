{
  "name": "parity.js",
  "version": "1.7.91",
  "main": "release/index.js",
  "jsnext:main": "src/index.js",
  "author": "Parity Team <admin@parity.io>",
  "maintainers": [
    "Jaco Greeff",
    "Nicolas Gotchac"
  ],
